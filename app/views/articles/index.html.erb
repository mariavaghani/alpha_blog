<h1>Articles listings</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Genre</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @article_list.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.description %></td>
      <td><%= article.genre %></td>
      <td>placeholder</td>
    </tr>
    <% end %>
  </tbody>
</table>
